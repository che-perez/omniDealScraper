{
    "version": 2,
    "buildCommand": "npm install && npm run build",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        },
        {
            "src": "/products",
            "dest": "dist/server.js"
        },
        {
            "src": "/scrape",
            "dest": "dist/server.js"
        },
        {
            "src": "/status",
            "dest": "dist/server.js"
        }
    ]
}